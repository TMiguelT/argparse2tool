#!/usr/bin/env cwl-runner

cwlVersion: "cwl:draft-3.dev2"

class: CommandLineTool
baseCommand: ["{{ basecommand }}"]

requirements:
  - class: InlineJavascriptRequirement

description: |
  {{ tool.description }}

inputs:
  {% block cwltool_inputs %}{% endblock %}

outputs:
  {% block cwltool_outputs %}
    []
  {% endblock %}